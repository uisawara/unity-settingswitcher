{
  "settings": [
    {
      "name": ".pc",
      "player_settings": {
        "virtualRealitySupported": false
      },
      "xr_settings": {
        "enabled": false
      },
      "scripting_define_symbols": "TARGET_PC"
    },
    {
      "name": ".vr",
      "player_settings": {
        "virtualRealitySupported": true
      },
      "xr_settings": {
        "enabled": true
      },
      "scripting_define_symbols": "TARGET_VR"
    },
    {
      "name": "flavors/SceneA",
      "inherit": ".pc",
      "scene_list": [
        "Assets/UnitySettingSwitcher/Samples/Scenes/SampleSceneA.unity",
        "Assets/UnitySettingSwitcher/Samples/Scenes/SampleSceneB.unity"
      ],
      "build_settings": {
        "build_target_group": "Standalone",
        "build_target": "StandaloneWindows64"
      },
      "player_settings": {
        "applicationIdentifier": "com.example.AppPc"
      }
    },
    {
      "name": "flavors/SceneB",
      "inherit": ".vr",
      "scene_list": [
        "Assets/UnitySettingSwitcher/Samples/Scenes/SampleSceneA.unity",
        "Assets/UnitySettingSwitcher/Samples/Scenes/SampleSceneC.unity"
      ],
      "build_settings": {
        "build_target_group": "Standalone",
        "build_target": "StandaloneWindows64"
      },
      "player_settings": {
        "applicationIdentifier": "com.example.AppVr"
      }
    },
    {
      "name": "buildtype/develop",
      "scripting_define_symbols": "ENV_DEVELOP"
    },
    {
      "name": "buildtype/production",
      "scripting_define_symbols": "ENV_PRODUCTION"
    }
  ]
}
